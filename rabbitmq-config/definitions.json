{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "notification-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "email.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 10,
        "x-message-ttl": 86400000,
        "x-max-length": 100000,
        "x-queue-type": "classic"
      }
    },
    {
      "name": "push.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 10,
        "x-message-ttl": 86400000,
        "x-max-length": 100000,
        "x-queue-type": "classic"
      }
    },
    {
      "name": "failed.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    }
  ],
  "exchanges": [
    {
      "name": "notifications.direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "notifications.direct",
      "vhost": "/",
      "destination": "email.queue",
      "destination_type": "queue",
      "routing_key": "email.queue",
      "arguments": {}
    },
    {
      "source": "notifications.direct",
      "vhost": "/",
      "destination": "push.queue",
      "destination_type": "queue",
      "routing_key": "push.queue",
      "arguments": {}
    }
  ]
}
